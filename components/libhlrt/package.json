{
  "name": "grebuloff-libhlrt",
  "description": "Grebuloff high-level runtime library",
  "version": "0.0.0",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist ../../build/libhlrt/",
    "build": "tsc && mkdirp ../../build/libhlrt && ncp package.json ../../build/libhlrt/package.json && ncp dist ../../build/libhlrt/dist"
  },
  "devDependencies": {
    "mkdirp": "^3.0.1",
    "ncp": "^2.0.0",
    "rimraf": "^5.0.1",
    "typescript": "^5.1.3"
  }
}